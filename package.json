{
  "name": "svalinn-discord-bot",
  "version": "4.0.0",
  "description": "Bot to manage events on discord",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy",
    "start": "ts-node ./lib/server.ts",
    "prod": "npm install; npm run build; forever start ./dist/server.js",
    "sonar": "tslint -t json -o report.json --project .; nyc npm test; node SonarCloud-Scanner.js",
    "test": "mocha --require ts-node/register tests/**/**/*.spec.ts --no-warnings --timeout 5000",
    "coverage": "nyc npm test",
    "copy": "cp -r lib/i18n dist/i18n"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Falcort/discord-bot-event.git"
  },
  "keywords": [
    "bot",
    "dicord",
    "event",
    "events"
  ],
  "author": "SOUQUET Thibault",
  "contributors": [
    "ROSAR Quentin",
    "MARTINEZ Jennifer"
  ],
  "license": "ISC",
  "dependencies": {
    "discord.js": "^11.4.2",
    "forever": "^1.0.0",
    "log4js": "^4.1.0",
    "luxon": "^1.12.1",
    "mocha-typescript": "^1.1.17",
    "mongoose": "^5.7.7",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "mocha": "^6.2.2",
    "chai": "^4.2.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/luxon": "^1.12.0",
    "@types/mongoose": "^5.5.23",
    "@types/node": "^11.15.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16",
    "sonarqube-scanner": "^2.4.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "lib"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "lines": 80,
    "sourceMap": true,
    "instrument": true,
    "clean": true
  }
}
